    <distributionManagement>
        <repository>
            <id>releases</id>
            <name>Internal Releases</name>
            <url>http://192.168.1.161:8081/repository/mz-releases/</url>
        </repository>
        <snapshotRepository>
            <id>snapshots</id>
            <name>Internal Snapshots</name>
            <url>http://192.168.1.161:8081/repository/mz-snapshots/</url>
        </snapshotRepository>
    </distributionManagement>
